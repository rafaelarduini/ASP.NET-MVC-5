
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>Valores obtidos do Home Controller via ViewData</h1>

        <div>
            <ul>
                @foreach (string strPais in (List<string>)ViewData["Paises"])
                {
                    <li>@strPais</li>
                }
            </ul>
        </div>
    </div>
</body>
</html>

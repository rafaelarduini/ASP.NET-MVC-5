@model BLL.Aluno
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Procurar</title>
</head>
<body>
    <div>
        <h4>Resultado da busca de Aluno</h4>
        <hr />
        @if (Model == null)
        {
            <dd>Nenhuma informação foi encontrada com o critério informado.</dd>
        }
        else
        {
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Nome)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.Email)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Email)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.Idade)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Idade)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.DataInscricao)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.DataInscricao)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.Sexo)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Sexo)
            </dd>
    
        </dl>
        }
    </div>
    <p>
        @Html.ActionLink("Retornar", "Index")
    </p>
</body>
</html>

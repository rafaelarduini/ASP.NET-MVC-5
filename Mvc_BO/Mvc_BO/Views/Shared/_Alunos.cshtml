@model BLL.Aluno

<table style="font-family:Arial; border:1px solid black; width:300px">
    <tr>
        <td>
            <img src="@Url.Content(Model.Foto)" alt="@Model.Texto" width="100" height="100" />
        </td>
        <td>
            <table>
                <tr>
                    <td><b>Nome</b></td>
                    <td><b>@Model.Nome</b></td>
                </tr>
                <tr>
                    <td><b>Idade</b></td>
                    <td><b>@Model.Idade</b></td>
                </tr>
                <tr>
                    <td><b>Inscrição</b></td>
                    <td><b>@Model.DataInscricao.ToString("dd/mm/yyyy")</b></td>
                </tr>
            </table>
        </td>
    </tr>
</table>


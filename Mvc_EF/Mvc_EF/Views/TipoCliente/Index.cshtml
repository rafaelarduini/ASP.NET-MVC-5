@model IEnumerable<Mvc_EF.Models.Tipo>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<h2>Lista de tipos de Cliente</h2>
<body>

    @foreach (var tipo in Model)
    {
        <li>@Html.ActionLink(tipo.Nome,"Index","Cliente", new { tipoId = tipo.TipoId}, null )        </li>
    }
</body>
</html>
